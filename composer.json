{
  "name": "mphpmaster/laravel-nova-helpers",
  "version": "1.2.0",
  "description": "Laravel Nova Helpers",
  "type": "library",
  "keywords": [
    "laravel",
    "functions",
    "helpers",
    "nova"
  ],
  "require": {
    "php": ">=8.1",
    "laravel/framework": ">=9",
    "illuminate/support": ">=9",
    "laravel/nova": "~4.0",
    "laravel/helpers": "^1.5"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "mPhpMaster",
      "email": "59211285+mPhpMaster@users.noreply.github.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MPhpMaster\\LaravelNovaHelpers\\": "src/"
    },
    "files": [
      "src/Helpers/FCheckers.php",
      "src/Helpers/FGetters.php",
      "src/Helpers/FHelpers.php",
      "src/Helpers/FFields.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "MPhpMaster\\LaravelNovaHelpers\\Providers\\HelperProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}
